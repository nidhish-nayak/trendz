{
	"root": true,
	"parser": "@typescript-eslint/parser",
	"plugins": ["@typescript-eslint", "prettier", "jest"],
	"extends": [
		"prettier",
		"eslint:recommended",
		"plugin:jest/recommended",
		"plugin:@typescript-eslint/recommended"
	],
	"rules": {
		"no-unused-vars": "off",
		"@typescript-eslint/no-unused-vars": "off",
		"prettier/prettier": 2
	},
	"env": {
		"browser": true,
		"node": true,
		"jest/globals": true,
		"commonjs": true,
		"es6": true
	}
}
